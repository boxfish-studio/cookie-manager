		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"node-sass": "^7.0.3",
		"prettier": "^2.7.1",
		"prettier-plugin-svelte": "^2.8.0",

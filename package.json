	"devDependencies": {
		"@sveltejs/adapter-auto": "1.0.0",
		"@sveltejs/kit": "1.0.7",
		"@sveltejs/package": "^1.0.2",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"node-sass": "^8.0.0",
		"prettier": "^2.8.2",
		"prettier-plugin-svelte": "^2.9.0",
		"sass": "^1.57.1",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.2",
		"svelte-preprocess": "^5.0.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.4"
	},
	"type": "module",
	"lint-staged": {
		"**/*.{js,ts,svelte,scss}": [
			"prettier --write"
		]
	}

{
	"name": "@boxfish-studio/cookie-managers",
	"private": true,
	"scripts": {
		"build": "turbo build",
		"dev": "turbo dev",
		"lint": "turbo lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"format-all": "prettier --write \".\"",
		"manypkg": "manypkg",
		"prepare": "husky install",
		"skcm": "turbo --filter \"@boxfish-studio/sveltekit-cookie-manager\"",
		"rck": "turbo --filter \"@boxfish-studio/react-cookie-manager\"",
		"demo-nextjs": "turbo --filter \"demo-nextjs\""
	},
	"dependencies": {
		"@manypkg/cli": "^0.21.4",
		"husky": "^8.0.3",
		"lint-staged": "^13.1.0",
		"prettier": "^3.2.5",
		"turbo": "^2.1.2",
		"typescript": "^5.4.5"
	},
	"packageManager": "pnpm@9.10.0",
	"engines": {
		"node": ">=16"
	}
}
